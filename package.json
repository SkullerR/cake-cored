{
    "name": "cake-cored",
    "displayName": "CakeCORED",
    "description": "creates a simple label on status bar that shows the Debug value and, when clicked, it changes the current CakePHP loaded project debug value.",
    "version": "0.0.2",
    "publisher": "Crhistian",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:/config/app.php",
        "workspaceContains:/app/Config/core.php"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.changeDebugValue",
                "title": "Change Cake Debug Value"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}